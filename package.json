{
  "name": "templating-cli",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon main.js",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint . --ignore-path package.json --fix"
    ]
  },
  "author": "Avi Avinav",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "inquirer": "^8.2.1",
    "nanospinner": "^1.0.0"
  }
}
